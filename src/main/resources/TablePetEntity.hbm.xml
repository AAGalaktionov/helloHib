<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hiber.TablePetEntity" table="table_pet" schema="public" catalog="postgres">
        <id name="namePet">
            <column name="name_pet" sql-type="varchar(50) []" length="50"/>
        </id>
        <property name="type">
            <column name="type" sql-type="varchar(50) []" length="50"/>
        </property>
    </class>
</hibernate-mapping>